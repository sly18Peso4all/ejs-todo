
   <%- include('header'); -%>
   
   <div class="box" id="heading">
        <h2> <%= listTitle %></h2>
      </div>
   

    <div class="box">
        
            <% for (let i=0; i<newListItems.length; i++) { %>
                <div class="item">
                    <input type="checkbox">
                <p><%= newListItems[i] %><p/>
                 </p>
                </div>   
                <% } %>
 
    
    <form action="/" method="POST">
      <input type="text" name="newItem" placeholder="Add item" autocomplete="off"/>
      <button type="submit" name="list" value=<%= listTitle %> >+</button>
    </form>
   </div>

 <%- include('footer'); -%>